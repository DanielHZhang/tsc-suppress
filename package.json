{
  "name": "tsc-suppress",
  "version": "1.0.0",
  "author": "Daniel Zhang",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "bin/**/*",
    "dist/**/*"
  ],
  "bin": {
    "tsc-suppress": "./bin/tsc-suppress"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "rm -rf .build && node lib/index.js -p tsconfig.test.json",
    "dev:help": "node lib/index.js --help",
    "watch": "rm -rf dist && tsc --project tsconfig.json --watch",
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "prepublishOnly": "yarn build",
    "version": "git add .",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@supersede/eslint-config": "^1.0.1",
    "@supersede/prettier-config": "^1.0.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.11.10",
    "eslint": "^7.11.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "typescript": ">=3.0.0"
  }
}
